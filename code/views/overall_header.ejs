<body>
<form name="form0" id="form0" method="get" action="main.php">
<% if(typeof hid != "undefined") { %><%- hid0 %><% } %>
</form>

<div id="debug" style="position:absolute"></div>

<div id="panel_header">
    <div class="header_left"><img src="/images/logo.png" style="margin-top:5px;margin-left:3px;"></div>
    <div class="header_right">
        <div style="float:right; cursor:pointer; margin-right:5px;" onclick="doLogout()"><img src="images/logout.png" title="Logout"></div>
        <div style="float:right; cursor:pointer; margin-top:15px;" onclick="doLogout()">&nbsp;Logout&nbsp;</div>
        <div style="float:right; margin-top:15px;">Hi <strong><% if(typeof username != "undefined") { %><%- username %><% } %></strong>&nbsp;|&nbsp;</div>
        <div style="clear:both;"></div>
    </div>
</div>
<div id="panel_top">
    <% topnav.forEach(function(topnav){ %>
        <div onclick="doNav('<%= topnav.link %>')" class="navbutton<% if(topnav.xid == mod.xid || (mod.parent && topnav.xid == mod.parent.xid)) { %>_active<% } %>"><%= topnav.name %></div>
    <% }) %>
    <div style="clear:both;"></div>
</div>

<table width="100%" cellpadding="0" cellspacing="0" border="0">
<tr>
	<% if(typeof show_subnav != "undefined") { %>
    <td valign="top" id="panel_left" class="display_cell">
    <div id="panel_left_div">
		<% subnav.forEach(function(subnav){ %>
			<div onclick="doMod('<%= subnav.link %>')" class="leftnav<% if(subnav.xid == mod.xid) { %>_active<% } %>"><%= subnav.name %></div>
		<% }) %>
        <div>&nbsp;</div>
    </div>
    </td>
	<% } %>
    <td valign="top" style="overflow: hidden;">
    <div id="panel_body">
